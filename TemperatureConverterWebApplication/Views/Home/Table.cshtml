@{
    ViewData["Title"] = "Table";
}

<div class="text-center">
    <h3>Celsius / Fahrenheit</h3>   
</div>

<table class="display" style="width:80%">
    <thead>
        <tr>
            <th>Celsius</th>
            <th>Fahrenheit</th>
        </tr>
    </thead>
</table>


<script>

    $(document).ready(function () {
        $('table').DataTable({
        ajax: {
            "url": "api/temp/GetTableData"
            }
        });
    });

</script>